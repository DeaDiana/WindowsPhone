<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.0">
  <CommandSet xml:lang="en-US">
    <CommandPrefix>Joker</CommandPrefix>
    <Example> Next joke </Example>

    <Command Name="NextJoke">
      <Example> Next Joke </Example>
      <ListenFor> [Next] Joke </ListenFor>
      <Feedback> Searching the joke... </Feedback>
      <Navigate />
    </Command>
  <Command Name="NextTongueTwister">
      <Example> Next Tongue-twister </Example>
      <ListenFor> [Next] Tongue-[twister] </ListenFor>
      <ListenFor> [Next] [Tongue]-twister </ListenFor>
      <Feedback> Searching the tongue-twister... </Feedback>
      <Navigate />
    </Command>
  </CommandSet>
</VoiceCommands>